<div class="row ">
  <!-- aca va el post + el trending -->
  <div class="col s9" style="padding: 0 45px 10px 25px;">
    <!-- este es el div del post -->
    {{#each postArray}}
    <div id="post">
      <div class="row">
        <div class="col s3">
          <strong>{{this.userid}}
            <br>
            {{this.rank}}</strong>
        </div>
        <div class="col s9">
          <p>{{this.TXT}}</p>
          <div style="width: 50%;"><img style="width: 100%;" src="{{this.imgFile}}" alt="Imagen del post" srcset="">
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col s12">
          <a href="#"><i class="fas fa-comment"></i>
            <!--{{comments}}--> 23</a>
          <a href="#"><i class="fas fa-certificate"></i>
            <!--{{postPoints}} -->3</a>
        </div>
      </div>
    </div>
    {{/each}}
  </div>
  <!-- aca va el trending -->
  <div class="row">

    <div id="sideNav" class="col s3 nav-wrapper right green darken-4 sidenav-fixed">
      <!-- Este es el div de los posteos mas puntuados -->
      <div class="nav-content carousel carousel-slider">
        <h5 style="text-align: center;">TOPS!</h5>
        <div id="test-swipe-1" class="black">
          <div style="width: 50%;"><img style="width: 100%;" src="./img/test.jpg" alt="Imagen del post" srcset=""></div>
          <p style="color: white;">Lorem ipsum dolor sit amet consectetur</p>

        </div>
      </div>



    </div>
  </div>
</div>